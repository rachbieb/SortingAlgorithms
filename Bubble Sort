void bubbleSort(vector<int>& arr) {
        // code is to swap the element if the current element is greater than the next element
        int n=arr.size();
        for(int i=0;i<n-1;i++){
            for(int j=0;j<n-1;j++){
                if(arr[j]>arr[j+1]){
                    
                    int temp=arr[j];
                    arr[j]=arr[j+1];
                    arr[j+1]=temp;
                }
            }
        }
    }
